html
  head
    //include ./../includes/head.jade
    style.
      table {
          border-collapse: collapse;
          width: 100%;
      }

      th, td {
          text-align: left;
          padding: 8px;
      }

      tr:nth-child(even){background-color: #f2f2f2}

      th {
          background-color: #4CAF50;
          color: white;
      }

  body
    div(class="section group")
      h2(style='text-align:center;')!= 'List of Registered Customers'
      table(id="adminTable")
        th= 'Name'
        th='Email'
        th='Status'
        th='subdomain'
        th='Url'
        th='Schedule'
        th='Created'
          each vl, idx in val
            tr
              td=vl.regis.name
              td=vl.regis.email
              td=vl.action
              td=vl.subdomain
              td
                a(target='_blank' href='http://registration.que.one/' + vl.uniqueid)='manage'
              td
                if vl.subdomain
                  a(target='_blank' href='http://' + vl.subdomain + '.que.one/' + vl.uniqueid)='schedule'
              td=moment(vl.createdat).format('YYYY-MM-DD HH:mm:ss')
    
